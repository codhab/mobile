.ui.grid
  .two.wide.mobile.column
    %a{href: "#{new_ticket_path}"}
      %i.big.arrow.left.icon
  .thirteen.wide.mobile.column.text-center
    .title-header #{@form.name}
    .sub-title #{@form.description}
.ui.grid.container
  .sixteen.wide.column
    .ui.message.info.text-center
      %b O question√°rio foi respondido com sucesso!
.ui.grid.container
  .column.content-form
    %table.ui.celled.table
      %tbody.black
        - @form.form_fields.where(allow: true).each do |field|
          %tr
            %td
            %td
              %b= field.label
              %br/
              - if field._file?
                Foto enviada
              - else
                = @form_value.store[field.name].humanize
    
        